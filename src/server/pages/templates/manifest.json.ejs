
{
   "short_name": "<%= website.get('title') %>",
   "name": "<%= website.get('title') %>",
<% if(favicon) { -%>
   "icons": [
    {
      "src": "<%- icon(favicon.get('path'), 'webp', { dpi: 1, fit: 'cover', w: 192, h: 192 }) %>",
      "type": "image/webp",
      "sizes": "192x192"
    },{
      "src": "<%- icon(favicon.get('path'), 'png', { dpi: 1, fit: 'cover', w: 192, h: 192 }) %>",
      "type": "image/png",
      "sizes": "192x192"
    },{
      "src": "<%- icon(favicon.get('path'), 'png', { dpi: 1, fit: 'cover', w: 192, h: 192, p: 46, bg: config.favicon_background_color.replace('#', '') }) %>",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },{
      "src": "<%- icon(favicon.get('path'), 'webp', { dpi: 1, fit: 'cover', w: 512, h: 512 }) %>",
      "type": "image/webp",
      "sizes": "512x512"
    },{
      "src": "<%- icon(favicon.get('path'), 'png', { dpi: 1, fit: 'cover', w: 512, h: 512 }) %>",
      "type": "image/png",
      "sizes": "512x512"
    }
  ],
<% } -%>
  "start_url": "/?source=pwa",
  "background_color": "#FF0000",
  "display": "standalone",
  "scope": "/",
  "theme_color": "#0000FF"
}
